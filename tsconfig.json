{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/contexts/*": ["src/contexts/*"],
      "@/functions/*": ["src/functions/*"],
      "@/reactPages/*": ["src/reactPages/*"],
      "@/providers/*": ["src/providers/*"],
      "@/stores/*": ["src/stores/*"],
      "@/stories/*": ["src/stories/*"],
      "@/types/*": ["src/types/*"],
      "@/styles/*": ["src/styles/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/pages/*": ["src/pages/*"],
    },
  },
  "include": ["src/**/*", ".storybook/**/*"],
} 